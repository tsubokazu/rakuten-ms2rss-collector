{
  "application": {
    "name": "楽天MS2RSS株価データコレクター",
    "version": "1.0.0",
    "author": "Claude Code"
  },
  "data_collection": {
    "max_bars_per_request": 3000,
    "supported_timeframes": [
      "1M", "2M", "3M", "4M", "5M", "10M", "15M", "30M", "60M",
      "2H", "4H", "8H", "D", "W", "M"
    ],
    "supported_markets": [
      "T", "JAX", "JNX", "CHJ"
    ],
    "default_market": "T"
  },
  "csv_output": {
    "datetime_format": "YYYY-MM-DD HH:MM:SS",
    "decimal_places": 2,
    "include_header": true,
    "encoding": "UTF-8",
    "filename_template": "{stock_code}_{timeframe}_{start_date}-{end_date}.csv"
  },
  "ui_settings": {
    "default_timeframe": "5M",
    "default_bars": 1000,
    "max_stocks_batch": 20
  },
  "logging": {
    "enabled": true,
    "level": "INFO",
    "max_log_files": 10,
    "max_file_size_mb": 10
  }
}